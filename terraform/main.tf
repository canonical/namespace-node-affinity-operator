resource "juju_application" "namespace_node_affinity" {
  charm {
    name     = "namespace-node-affinity"
    channel  = var.channel
    revision = var.revision
  }
  config    = var.config
  model     = var.model_name
  name      = var.app_name
  resources = var.resources
  trust     = true
  units     = 1
}
